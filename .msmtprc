defaults
	tls  on
	tls_trust_file /etc/ssl/certs/ca-certificates.crt

account palmerdabbelt@google.com
	host smtp.gmail.com
	port 587
	from         palmerdabbelt@google.com
	user         palmerdabbelt@google.com
	auth         oauthbearer
	passwordeval "mhng-pipe-oauthbearer palmerdabbelt@google.com"

account palmer@dabbelt.com
	host smtp.gmail.com
	port 587
	from         palmer@dabbeltcom
	user         palmer@dabbeltcom
	auth         oauthbearer
	passwordeval "mhng-pipe-oauthbearer palmer@dabbelt.com"

account default: palmerdabbelt@google.com
